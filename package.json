{
  "name": "nodejs-scaffolding",
  "version": "1.0.0",
  "description": "A node.js sample application",
  "main": "index.js",
  "scripts": {
    "add": "all-contributors add",
    "dev": "brew services start mongodb-community@4.4 && nodemon index.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "generate": "all-contributors generate",
    "jshint": "jshint src/. test/. --config",
    "mocha": "mocha test/ --recursive",
    "start": "node index.js",
    "test": "npm run jshint && npm run mocha"
  },
  "repository": "https://github.com/khriztianmoreno/nodejs-scaffolding.git",
  "keywords": [
    "api",
    "node",
    "mobi",
    "express",
    "mongodb",
    "passport"
  ],
  "author": "Cristian Moreno <khriztianmoreno@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khriztianmoreno/nodejs-scaffolding/issues"
  },
  "homepage": "https://github.com/khriztianmoreno/nodejs-scaffolding/blob/master/README.md",
  "engines": {
    "node": "^6.9.x"
  },
  "dependencies": {
    "async": "^3.2.0",
    "azure-storage": "^2.10.3",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "composable-middleware": "^0.3.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "lusca": "^1.6.1",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "mongoose": "^5.11.15",
    "mongoose-paginate": "^5.0.3",
    "morgan": "~1.10.0",
    "nodemailer": "^6.4.18",
    "nodemailer-direct-transport": "^3.3.2",
    "nodemailer-express-handlebars": "^4.0.0",
    "passport": "~0.4.1",
